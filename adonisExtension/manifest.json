{
  "manifest_version": 2,
  "name": "Adonis Extension",
  "version": "1.1.0",
  "icons":
   {
      "128": "adonis.png"
   },
  "browser_action": {
    "default_icon": "adonis.png"
  },
  "background" : {
    "scripts" : ["background.js"]
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://ajax.googleapis.com/",
    "cookies",
    "<all_urls>",
    "notifications"
  ],
  "incognito": "split"
}
